
group "0.operator" {

  cmd "0.drake-director" {
    exec = "bazel-bin/director/drake-director --script examples/Cassie/pd_panel.py";
    host = "localhost";
  }

  cmd "1.state-visualizer" {
    exec = "bazel-bin/examples/Cassie/cassie_state_visualizer";
    host = "localhost";
  }
}

group "1.robot" {
  cmd "0.pd-controller" {
    exec = "bazel-bin/examples/Cassie/run_pd_controller";
    host = "localhost";
  }

  cmd "1.simulator" {
    exec = "bazel-bin/examples/Cassie/run_simple_sim";
    host = "localhost";
  }

  cmd "2.fake-state-pub" {
    exec = "bazel-bin/examples/Cassie/run_state_pub_test";
    host = "localhost";
  }
}

group "2.lcm-spy" {
  cmd "0.lcm-spy" {
    exec = "bazel-bin/lcmtypes/cassie-lcm-spy";
    host = "localhost";
  }

}
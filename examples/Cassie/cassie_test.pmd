
group "0.operator" {

  cmd "0.drake-visualizer" {
    exec = "../drake/bazel-bin/tools/drake_visualizer";
    host = "localhost";
  }

  cmd "2.pd-gui" {
    exec = "bazel-bin/examples/Cassie/pd_controller_gui";
    host = "localhost";
  }

  cmd "3.state-visualizer" {
    exec = "bazel-bin/examples/Cassie/cassie_state_visualizer";
    host = "localhost";
  }
}

group "1.robot" {
  cmd "0.pd-controller" {
    exec = "bazel-bin/examples/Cassie/run_pd_controller";
    host = "localhost";
  }

  cmd "1.fake-state-pub" {
    exec = "bazel-bin/examples/Cassie/run_state_pub_test";
    host = "localhost";
  }
}

group "2.lcm-spy" {
  cmd "0.lcm-spy" {
    exec = "bazel-bin/lcmtypes/cassie-lcm-spy";
    host = "localhost";
  }

}